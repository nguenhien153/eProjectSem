@model IEnumerable<jwelleryshopping.Models.BrandMst>

@{
    ViewBag.Title = "Brand";
    Layout = "~/Views/Shared/_LayoutHomeAdmin.cshtml";
}

<h2>Brand</h2>

<p>
    @Html.ActionLink("Create New", "AddBrand","Admin")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Brand_Type)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Brand_Type)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditBrand", new { id=item.Brand_ID }) |
            @Html.ActionLink("Details", "DetailsBrand", new { id=item.Brand_ID }) |
            @Html.ActionLink("Delete", "DeleteBrand", new { id=item.Brand_ID })
        </td>
    </tr>
}

</table>
